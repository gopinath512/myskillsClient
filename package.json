    "@ng-select/ng-select": "^9.0.2",
    "@ngx-translate/core": "^14.0.0",
    "@popperjs/core": "^2.11.5",
    "@swimlane/ngx-datatable": "^20.0.0",
    "angular-archwizard": "^7.0.0",
    "angular-oauth2-oidc": "^13.0.1",
    "apexcharts": "^3.49.0",
    "bootstrap": "^5.2.0",
    "bootstrap-icons": "^1.11.3",
    "bootswatch": "^5.2.0",
    "chart.js": "^3.8.2",
